{
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "guzzlehttp/guzzle": "^7.5",
    "hashids/hashids": "^4.1",
    "jamband/ripple": "^0.14.0",
    "laravel/framework": "^9.45"
  },
  "require-dev": {
    "brianium/paratest": "^6.7",
    "fakerphp/faker": "^1.21",
    "friendsofphp/php-cs-fixer": "^3.13",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^6.3",
    "nunomaduro/larastan": "^2.3",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
    "dev": "@composer i -q",
    "format": "php-cs-fixer fix --dry-run",
    "analyse": "phpstan analyse --memory-limit=2G --xdebug",
    "serve": "@php -S 127.0.0.1:8000 -t public",
    "clean": "@php artisan dev:clean && rm -rf vendor"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
